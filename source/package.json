{
  "name": "document-understanding-solution-demo",
  "version": "2.0.0",
  "description": "A web app and Cloud Formation template that demonstrates the capabilities of the Amazon Textract, Comprehend and Comprehend Medical.",
  "main": "handler.js",
  "repository": {
    "type": "git"
  },
  "license": "Apache-2.0",
  "private": true,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "Firefox ESR"
  ],
  "stack": {
    "stackname": "DUS",
    "region": "%%REGION%%"
  },
  "cicd": "false",
  "isROMode": "false",
  "enableKendra": "false",
  "email": "%%USER_EMAIL%%",
  "scripts": {
    "bootstrap": "AWS_REGION=$npm_package_stack_region USER_EMAIL=$npm_package_email cdk bootstrap --toolkit-stack-name DocumentUnderstandingCDKToolkit",
    "compile-ts-stacks": "yarn compile-ts-backend-stack && yarn compile-ts-client-stack",
    "compile-ts-backend-stack": "tsc lib/cdk-textract-stack.ts --target es2018 --module commonjs --allowjs true",
    "compile-ts-client-stack": "tsc lib/cdk-textract-client-stack.ts --target es2018 --module commonjs --allowjs true",
    "deploy:backend": "ISCICD=$npm_package_cicd ENABLE_KENDRA=$npm_package_enableKendra STACKNAME=$npm_package_stack_stackname AWS_REGION=$npm_package_stack_region USER_EMAIL=$npm_package_email cdk deploy --toolkit-stack-name DocumentUnderstandingCDKToolkit --require-approval never -v true",
    "deploy:client": "yarn export && yarn update-es-logs-and-stack-variables && AWS_REGION=$npm_package_stack_region STACKNAME=$npm_package_stack_stackname cdk deploy --toolkit-stack-name DocumentUnderstandingCDKToolkit --require-approval never -v true -a \"node bin/deploy-client-stack.js\"",
    "deploy:setup-user": "AWS_REGION=$npm_package_stack_region node bin/user-setup.js",
    "deploy:setup-samples": "AWS_REGION=$npm_package_stack_region ENABLE_KENDRA=$npm_package_enableKendra bash ./bin/upload-samples.sh",
    "deploy-all": "yarn add-service-linked-role && yarn run:tests && yarn compile-ts-stacks && yarn bootstrap && yarn deploy:backend && yarn deploy:client && yarn deploy:setup-samples",
    "deploy": "yarn install-all && yarn deploy-all && yarn update-pdf-lambda-code",
    "deploy:frontend": "yarn deploy:client && yarn deploy:setup-samples",
    "destroy": "STACKNAME=$npm_package_stack_stackname AWS_REGION=$npm_package_stack_region cdk destroy -v true -a \"node bin/deploy-client-stack.js\" && STACKNAME=$npm_package_stack_stackname AWS_REGION=$npm_package_stack_region USER_EMAIL=$npm_package_email cdk destroy -v true",
    "update-es-logs-and-stack-variables": "bash ../deployment/custom-deployment/bin/update-es-logs-and-client-stack-vars.sh",
    "update-pdf-lambda-code": "bash ../deployment/custom-deployment/bin/update-pdf-lambda-code.sh",
    "dev": "NEXT_PUBLIC_ENABLE_KENDRA=$npm_package_enableKendra next app",
    "build": "STACKNAME=$npm_package_stack_stackname AWS_REGION=$npm_package_stack_region yarn pre-build && NEXT_PUBLIC_ENABLE_KENDRA=$npm_package_enableKendra next build app",
    "export": "yarn build && next export app",
    "pre-build": "STACKNAME=$npm_package_stack_stackname AWS_REGION=$npm_package_stack_region isROMode=$npm_package_isROMode ENABLE_KENDRA=$npm_package_enableKendra node ./bin/pre-build.js",
    "license-report": "legally --plain > license-report.txt && echo \"$(tail -n +4 license-report.txt)\" > license-report.txt",
    "prettier": "prettier --write '**/*.{js,md,yaml}'",
    "run:tests": "bash bin/run-tests.sh",
    "install-all": "bash ../deployment/install_packages.sh",
    "add-service-linked-role": "bash ../deployment/custom-deployment/bin/add_es_service_role_if_missing.sh",
    "test-client-build": "next build app"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "1.158.0",
    "@aws-cdk/aws-cloudformation": "1.158.0",
    "@aws-cdk/aws-cognito": "1.158.0",
    "@aws-cdk/aws-dynamodb": "1.158.0",
    "@aws-cdk/aws-ec2": "1.158.0",
    "@aws-cdk/aws-elasticsearch": "1.158.0",
    "@aws-cdk/aws-events": "1.158.0",
    "@aws-cdk/aws-iam": "1.158.0",
    "@aws-cdk/aws-lambda": "1.158.0",
    "@aws-cdk/aws-lambda-event-sources": "1.158.0",
    "@aws-cdk/aws-logs": "1.158.0",
    "@aws-cdk/aws-s3": "1.158.0",
    "@aws-cdk/aws-s3-deployment": "1.158.0",
    "@aws-cdk/aws-s3-notifications": "1.158.0",
    "@aws-cdk/aws-sns": "1.158.0",
    "@aws-cdk/aws-sqs": "1.158.0",
    "@aws-cdk/core": "1.158.0",
    "@babel/helper-validator-identifier": "^7.18.6",
    "@types/node": "^18.6.1",
    "aws-amplify": "^4.3.28",
    "aws-cdk": "1.158.0",
    "aws-sdk": "^2.1182.0",
    "classnames": "^2.3.1",
    "date-fns": "^1.30.1",
    "dotenv": "^16.0.1",
    "express": "^4.18.1",
    "inquirer": "^9.0.2",
    "lodash": "^4.17.21",
    "minimist": "^1.2.6",
    "next": "12.2.4",
    "next-redux-wrapper": "5.0.0",
    "node-fetch": "^3.2.10",
    "normalizr": "^3.6.2",
    "query-string": "^7.1.1",
    "ramda": "^0.28.0",
    "react": "17.0.2",
    "react-dom": "18.2.0",
    "react-dropzone": "^14.2.2",
    "react-highlighter": "^0.4.3",
    "react-intersection-observer": "^9.4.0",
    "react-joyride": "^2.5.0",
    "react-pdf": "^4.1.0",
    "react-redux": "^8.0.2",
    "react-webcam": "^7.0.1",
    "readline-sync": "^1.4.10",
    "redux": "^4.2.0",
    "redux-actions": "^2.6.5",
    "redux-devtools-extension": "^2.13.9",
    "redux-promise": "^0.6.0",
    "redux-thunk": "^2.4.1",
    "sass": "^1.54.0",
    "serialize-javascript": "^6.0.0",
    "short-uuid": "^4.2.0",
    "susy": "^3.0.7",
    "typescript": "^4.7.4"
  },
  "devDependencies": {
    "autoprefixer": "^10.4.7",
    "babel-eslint": "^10.0.1",
    "eslint": "^8.20.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.30.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "legally": "^3.5.10",
    "prettier": "^2.7.1"
  }
}
